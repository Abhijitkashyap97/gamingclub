{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "headers": {
        "Content-Type": "text/css"
      },
      "dest": "/public/styles.css"
    },
    {
      "src": "/about_page",
      "dest": "/index.js"
    },
    {
      "src": "/games_parti",
      "dest": "/index.js"
    },
    {
      "src": "/tournaments",
      "dest": "/index.js"
    },
    {
      "src": "/query",
      "dest": "/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
