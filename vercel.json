{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/about_page",
      "dest": "/index.js"
    },
    {
      "src": "/games_parti",
      "dest": "/index.js"
    },
    {
      "src": "/tournaments",
      "dest": "/index.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/public/$1.css"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}